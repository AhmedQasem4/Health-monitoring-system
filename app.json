{
  "expo": {
    "name": "HMSMobileApp",
    "slug": "HMSMobileApp",
    "version": "1.0.0",
    "notification": {
      "icon": "./app/assets/logo.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new messages"
    },
    "orientation": "portrait",
    "icon": "./app/assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ahmedqasem.HMSMobileApp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5afcd7bf-9e9d-4783-a22e-1a2e1fba5b31"
      }
    },
    "plugins": [
      "react-native-ble-plx"
    ]
  }
}
